{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layout/*": ["src/_layout/*"],
      "@parts/*": ["src/_parts/*"],
      "@partner/*": ["src/_parts/partner/*"]
    },
    "types": ["@cloudflare/workers-types"]
  }
}
