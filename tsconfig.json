{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist", "src/scripts"],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@layout/*": ["src/layout/*"],
      "@parts/*": ["src/parts/*"],
      "@partner/*": ["src/parts/partner/*"]
    },
    "types": ["@cloudflare/workers-types"]
  }
}
