{
  "name": "hotdogwalk-com",
  "type": "module",
  "version": "0.0.1",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "prepublishOnly": "npm run make",
    "publish": "wrangler pages deploy",
    "dev": "wrangler types; wrangler pages dev --live-reload",
    "make": "astro build",
    "test": "stylelint --fix src/_css; eslint --fix src/_ts functions; tsc --noEmit",
    "watch": "chokidar 'public/**/*' 'src/**/*' -c 'npm run make'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.1.0",
    "@astrojs/sitemap": "^3.2.1",
    "astro": "^5.1.4",
    "resend": "^4.1.2",
    "stripe": "^17.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@eslint/js": "^9.21.0",
    "chokidar-cli": "^3.0.0",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.5.3",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "wrangler": "^3.100.0"
  }
}
