{
  "name": "hotdogwalk-com",
  "version": "1.0.0",
  "type": "module",
  "license": "BSD-3-Clause",
  "browserslist": [
    "defaults"
  ],
  "scripts": {
    "posttest": "stylelint --custom-formatter node_modules/stylelint-sarif-formatter/index.js -o .sarif/stylelint.sarif src/styles; eslint -f @microsoft/eslint-formatter-sarif -o .sarif/eslint.sarif src/scripts functions",
    "prepare": "astro build",
    "prestart": "astro build",
    "pretest": "astro build",
    "publish": "wrangler pages deploy",
    "start": "wrangler types; wrangler pages dev --live-reload",
    "test": "stylelint --fix src/styles; eslint --fix src/scripts functions; tsc --noEmit",
    "watch": "chokidar 'public/**/*' 'src/**/*' -c 'astro build'"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.2.3",
    "@astrojs/mdx": "^4.1.0",
    "@astrojs/sitemap": "^3.2.1",
    "astro": "^5.1.4",
    "resend": "^4.1.2",
    "stripe": "^17.7.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@eslint/js": "^9.21.0",
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "chokidar-cli": "^3.0.0",
    "cssnano": "^7.0.6",
    "cssnano-preset-advanced": "^7.0.6",
    "postcss-preset-env": "^10.1.3",
    "prettier": "^3.4.2",
    "stylelint": "^16.14.1",
    "stylelint-config-standard": "^37.0.0",
    "stylelint-sarif-formatter": "^1.0.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.25.0",
    "wrangler": "^3.100.0"
  }
}
